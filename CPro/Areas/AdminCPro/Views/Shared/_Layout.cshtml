
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Admin CPro</title>

    <!-- Bootstrap Core CSS -->
    <link href="/AssetCPro/Admin/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="/AssetCPro/Admin/bower_components/metisMenu/dist/metisMenu.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="/AssetCPro/Admin/dist/css/sb-admin-2.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="/AssetCPro/Admin/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link rel="icon" href="~/AssetCPro/Admin/image/personal.ico"/>

</head>

<body>

    <div id="wrapper">

        <!-- Navigation -->
       @RenderPage("_Navigation.cshtml")

        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <h2 class="page-header" style="text-align: center">@RenderSection("header",required:true)</h2>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                @RenderBody()
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
    <!-- jQuery -->

    <script src="/Scripts/jquery-1.10.2.min.js"></script>
    <script src="/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="/AssetCPro/Admin/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="/AssetCPro/Admin/bower_components/metisMenu/dist/metisMenu.min.js"></script>

    <script src="~/AssetCPro/Admin/ckfinder/ckfinder.js"></script>
    <script src="~/AssetCPro/Admin/ckeditor/ckeditor.js"></script>
    <!-- Custom Theme JavaScript -->
<script src="/AssetCPro/Admin/dist/js/sb-admin-2.js"></script>

    @RenderSection("jsFooter",required:false)
</body>
</html>

